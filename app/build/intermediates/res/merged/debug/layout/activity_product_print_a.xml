<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:fitsSystemWindows="true"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include layout="@layout/common_title"/>
    <ScrollView
        android:background="#eae6e6"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="检疫证编号:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_no"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:hint="请输入检疫证编号"
                    android:inputType="number"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:maxLength="10"
                    android:textSize="@dimen/common_et_size"
                    android:text="43" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="货  主:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_user_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size"
                    android:layout_weight="1" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#3399cc"
                    android:text="联系电话:"
                    android:layout_gravity="center"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_user_phone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size"
                    android:layout_weight="1" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#3399cc"
                    android:text="产品名称:"
                    android:layout_gravity="center"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_product_type"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/s1"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="数量及单位:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size"
                    android:layout_weight="1" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="生产单位名称:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_production_unit"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:hint="请输入生产单位名称"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size"/>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#3399cc"
                    android:text="起运地:"
                    android:layout_gravity="center"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_start_place"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="@drawable/s1"
                    android:textSize="12dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="目的地:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_end_place"
                    android:layout_width="0dp"
                    android:background="@drawable/s1"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size"
                    android:layout_weight="1" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="承运人:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_carrier_name"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:background="@drawable/s1"
                    android:hint="请输入承运人！！"
                    android:gravity="center"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size"
                    android:layout_weight="1" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系电话:"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_carrier_phone"
                    android:layout_width="0dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:background="@drawable/s1"
                    android:hint="请输入联系电话!!"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"
                    android:maxLength="13"
                    android:digits="0123456789-"
                    android:inputType="number"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="运输方式:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_method"
                    android:layout_width="0dp"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"
                    android:layout_height="27dp"
                    android:background="@drawable/s1"
                    android:layout_weight="1" />


            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="运输工具号:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_util_number"
                    android:layout_width="0dp"
                    android:layout_height="27dp"
                    android:hint="请输入运输工具号!!"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"
                    android:background="@drawable/s1"
                    android:layout_weight="1" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="装载前经:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_sterilize"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:focusable="false"
                    android:textSize="@dimen/common_et_size"
                    android:hint="请输入消毒方式"
                    android:textColorHint="#FF6666"
                    android:gravity="center"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="消毒"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="有效到达日:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <Spinner
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:id="@+id/spinner_effect_arrive_time"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="备注:"
                    android:layout_marginRight="10dp"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_remark"
                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"
                    android:background="@drawable/s1"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="签发日期:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_qianfa_time"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="动物卫生监督所电话:"
                    android:layout_gravity="center"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_jiandu_phone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="请输入动物监督所联系电话"
                    android:inputType="number"
                    android:gravity="center"
                    android:maxLength="13"
                    android:layout_weight="1"
                    android:background="@drawable/s1"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="40dp" >
                <Button
                    android:id="@+id/bt_print"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="打印"
                    android:background="@drawable/bt_bg_blue"
                    android:textColor="#ffffff"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
