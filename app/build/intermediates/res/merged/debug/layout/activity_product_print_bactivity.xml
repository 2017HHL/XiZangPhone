<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:layout_height="match_parent">
    <include layout="@layout/common_title"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="检疫证编号:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="请输入检疫证编号"
                    android:background="@drawable/s1"
                    android:inputType="number"
                    android:maxLength="10"
                    android:gravity="center"
                    android:text="43"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="货   主  : "
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_user_name"
                    android:layout_width="match_parent"
                    android:background="@drawable/s1"
                    android:layout_height="30dp"
                    android:textSize="@dimen/common_et_size"
                    android:gravity="center"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系电话:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_user_phone"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:textSize="@dimen/common_et_size"
                    android:background="@drawable/s1"
                    android:gravity="center"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="产品名称:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_product_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="@drawable/s1"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="数量及单位:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_number"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="生产单位名称:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_production_unit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:hint="请输入生产单位名称"
                    android:textColorHint="#FF6666"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="地址:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_start_place"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>

            <!--启运地的市县-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="产 地 :"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_product_place"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="目的地 :"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_end_place"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="检疫标志号 :"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_flag_jianyi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/s1"
                    android:hint="选填"
                    android:textColorHint="#FF6666"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:text="运输方式:"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tvTransport3"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:text="运输工具号:"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tvVehicleNo4"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:text="装载前经:"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tvDeliveryBefore4"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="签发日期:"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <TextView
                    android:id="@+id/tv_qianfa_time"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:textSize="@dimen/common_et_size"
                    android:background="@drawable/s1"
                    android:gravity="center" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="70dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:text="官方兽医签字："
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tvqzi3"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginRight="15dp"
                    android:textSize="15dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:text="备   注  :"
                    android:textColor="#3399cc"
                    android:textSize="@dimen/common_tv_size_title" />

                <EditText
                    android:id="@+id/et_remark"
                    android:textColorHint="#FF6666"
                    android:hint="选填"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="@drawable/s1"
                    android:gravity="center"
                    android:textSize="@dimen/common_et_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="10dp" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:text="动物卫生监督所电话:"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tvnumber4"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp" />
            </LinearLayout>

            <!--<LinearLayout-->
                <!--android:id="@+id/TakePhotosa"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:orientation="horizontal"-->
                <!--android:weightSum="1">-->

                <!--<Button-->
                    <!--android:id="@+id/TakeB2"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="40dp"-->
                    <!--android:layout_marginLeft="60dp"-->
                    <!--android:textSize="12dp"-->
                    <!--android:background="#3399cc"-->
                    <!--android:textColor="#ffffff"-->
                    <!--android:onClick="onClick"-->
                    <!--android:text="拍照"-->
                    <!--/>-->


                <!--<ImageView-->
                    <!--android:id="@+id/imagev"-->
                    <!--android:layout_width="50dp"-->
                    <!--android:layout_height="103dp"-->
                    <!--android:tag="PT1"-->
                    <!--android:layout_marginLeft="20dp"-->
                    <!--android:layout_weight="0.11" />-->


            <!--</LinearLayout>-->

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >
                <Button
                    android:id="@+id/bt_print"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/bt_bg_blue"
                    android:textColor="#ffffff"
                    android:text="打印"
                    android:textSize="@dimen/common_tv_size" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
